{
    "name": "christopherokonkwo/laravel-paystack",
    "description": "A Laravel 5, 6 and 7 Package for Paystack",
    "keywords": ["php","github", "laravel","Open Source","payments", "subscription", "paystack", "paystack.co","laravel 5"],
    "license": "MIT",
    "authors": [
      {
        "name": "unicodeveloper",
        "email": "prosperotemuyiwa@gmail.com"
      },
      {
        "name": "iamfunsho",
        "email": "info@devfunsho.com"
      },
      {
        "name": "christopher okonkwo",
        "email": "xtopherc43@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": "^5.4.0|^7.0",
    "illuminate/support": "5.*|^6|^7",
    "guzzlehttp/guzzle": "5.*|^6|^7"
   },
   "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "satooshi/php-coveralls": "^0.7.0",
        "mockery/mockery": ">=0.7.2"
    },
   "autoload": {
        "psr-4": {
          "ChristopherOkonkwo\\Paystack\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "ChristopherOkonkwo\\Paystack\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
   "extra": {
    "laravel": {
        "providers": [
            "ChristopherOkonkwo\\Paystack\\PaystackServiceProvider"
        ],
        "aliases": {
            "Paystack": "ChristopherOkonkwo\\Paystack\\Facades\\Paystack"
        }
    }
  }
}
